{
    "functions": [
        {
            "name": "type",
            "description": "Get the type.",
            "examples": ["(type 50) ;integer"],
            "return": { "type": "atom" },
            "args": [[{ "type": "expression" }]]
        },
        {
            "name": "integer->float",
            "description": "Convert an integer to a float.",
            "examples": ["(integer->float 50) ;50.0"],
            "return": { "type": "float" },
            "args": [[{ "type": "integer" }]]
        },
        {
            "name": "float->integer",
            "description": "Convert a float to an integer.",
            "examples": ["(float->integer 50.5) ;50"],
            "return": { "type": "integer" },
            "args": [[{ "type": "float" }]]
        }
    ]
}
